{%- for section, settings in config|dictsort %}
[{{ section }}]
    {%- for setting, value in settings|dictsort %}
        {% if value is number or value is string -%}
{{ setting }} = {{ value }}
        {%- elif value is iterable -%}
            {%- if setting == 'error_reporting' %}
{{ setting }} = {{ value|join(" & ") }}
            {%- else -%}
{{ setting }} = {{ value|join(",") }}
            {%- endif -%}
        {%- endif %}
    {%- endfor %}
{% endfor %}
